@model TreinamentoWeb.Models.Aluno

<div class="col-md-24">
    <form>

        <div class="col-md-12">
            @Html.Label("Nome")
            @Html.TextBoxFor(x => x.Nome, null, new { @class = "form-control", disabled = "disabled" })
        </div>
        <div class="col-md-11 col-md-offset-1">
            @Html.Label("CPF")
            @Html.TextBoxFor(x => x.Cpf, null, new { @class = "form-control", disabled = "disabled" })
        </div>

    </form>
</div>